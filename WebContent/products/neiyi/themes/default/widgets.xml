		</widget>	
	</page>

	<page id="/index.html">
		<widget id="jqueryAd">
			<acid>2</acid>
			<overflow>visible</overflow>
			<bordertitle />
			<border>none</border>
			<type>jqueryAdAdvance</type>
		</widget>
		<widget id="news1">
			<type>staticDataList</type>
			<count>4</count>
			<catid>8</catid>
			<folder>customPage</folder>
			<custom_page>index_news</custom_page>
		</widget>
		<widget id="news2">
			<type>staticDataList</type>
			<count>4</count>
			<catid>9</catid>
			<folder>customPage</folder>
			<custom_page>index_news</custom_page>
		</widget>
		<widget id="ad1">
			<acid>3</acid>
			<overflow>visible</overflow>
			<bordertitle />
			<border>none</border>
			<type>ad</type>
		</widget>
		<widget id="hot">
			<bordertitle>热卖商品</bordertitle>
			<border>hot</border>
			<type>goods_list</type>
			<term>{tagid:'1'}</term>
			<setting>{type:'default',goodsImgPosition:'top',
				thumbnail_pic_width:'190px',thumbnail_pic_height:'245px',
				showGoodsName:'on', goodsNum:8}</setting>
			<overflow>visible</overflow>
			<folder>customPage</folder>
			<custom_page>index_goodsList</custom_page>
		</widget>
		<widget id="ad2">
			<acid>4</acid>
			<overflow>visible</overflow>
			<bordertitle />
			<border>none</border>
			<type>ad</type>
		</widget>
		<widget id="bra">
			<bordertitle></bordertitle>
			<border>none</border>
			<type>goods_list</type>
			<overflow>visible</overflow>
			<tabs>
				[
				{title:'魔术系列',term:{catid:'6'}},
				{title:'挺拔集中',term:{catid:'7'}},
				{title:'轻薄透气',term:{catid:'8'}}
				]
			</tabs>
			<setting>{type:'morediv',goodsImgPosition:'top',changeEffect:0,columnnum:5,goodsNum:10}</setting>
			<folder>customPage</folder>
			<custom_page>index_bra</custom_page>
		</widget>
		<widget id="recommend">
			<bordertitle>精品推荐</bordertitle>
			<border>recommend</border>
			<type>goods_list</type>
			<term>{tagid:'3'}</term>
			<setting>{type:'default',goodsImgPosition:'top',
				thumbnail_pic_width:'180px',thumbnail_pic_height:'232px',
				showGoodsName:'on', columnnum:5, goodsNum:10}</setting>
			<overflow>visible</overflow>
			<folder>customPage</folder>
			<custom_page>index_goodsList</custom_page>
		</widget>
		<widget id="fadeplay">
			<type>fadeplay</type>
			<acid>6</acid>
			<custom_page>squareAd</custom_page>
			<folder>customPage</folder>
			<overflow>visible</overflow>
		</widget>
	</page>

