<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<widgets>
	<page id="common">
		<widget id="header">
			<type>header</type>
			<border>none</border>
		</widget>
		<widget id="loginbar">
			<type>member_login_bar</type>
