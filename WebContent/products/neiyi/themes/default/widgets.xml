		</widget>
		<widget id="searchbar">
			<type>searchBar</type>
			<border>none</border>
		</widget>
		<widget id="logo">
			<type>imageWidget</type>
			<src>attachment/images/logo.png</src>
		</widget>
		<widget id="banner">
			<type>ad</type>
			<acid>1</acid>
		</widget>
		<widget id="category">
			<overflow>visible</overflow>
			<bordertitle>商品分类</bordertitle>
			<border>border1</border>
			<type>goods_cat</type>
